{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ES2015",
    "target": "ES5",
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@mockData/*": ["src/mockData/*"],
      "@sutils/*": ["src/utils/*"],
      "@stypes/*": ["src/types/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
